import string
import random

def generate():
    password_char = []
    # Get alphabets

    for i in range(4):
        letters = random.choice(string.ascii_letters)
        password_char.append(letters)
 
        digits = random.choice(string.digits)
        password_char.append(digits)             

        punctuation =  random.choice(string.punctuation)
        password_char.append(punctuation)


    random.shuffle(password_char)
    password = ''.join(password_char) 
    return password
